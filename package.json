{
  "name": "doki-doki-mod-manager",
  "productName": "Doki Doki Mod Manager!",
  "description": "Mod Manager for Doki Doki Literature Club!",
  "author": "u/zuudo",
  "version": "2.15.0",
  "homepage": "https://doki.space",
  "license": "MIT",
  "main": "src/common/main.js",
  "scripts": {
    "start": "electron .",
    "start-dev": "electron --inspect=5858 .",
    "release": "electron-builder"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@sentry/electron": "latest",
    "@zudo/unzipper": "0.0.4",
    "bytes": "^3.0.0",
    "chalk": "^2.4.1",
    "chmodr": "latest",
    "discord-rich-presence": "^0.0.7",
    "electron-is-dev": "latest",
    "electron-updater": "^4.0.1",
    "fs-extra": "latest",
    "fuse.js": "^3.2.1",
    "markdown": "^0.5.0",
    "mousetrap": "^1.6.2",
    "request": "^2.87.0",
    "sanitize-filename": "latest",
    "semver": "latest",
    "sha": "latest",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@types/request": "^2.47.1",
    "devtron": "^1.4.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.26.1"
  },
  "build": {
    "appId": "space.doki.modmanager",
    "artifactName": "ddmm-${version}-${os}-${arch}.${ext}",
    "publish": [
      {
        "provider": "github",
        "repo": "Mod-Manager",
        "owner": "DokiDokiModManager"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "category": "Game",
      "icon": "build/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "category": "public.app-category.games",
      "target": [
        "dmg"
      ]
    }
  }
}
