{
  "name": "doki-doki-mod-manager",
  "productName": "Doki Doki Mod Manager",
  "description": "Mod Manager for Doki Doki Literature Club",
  "author": "Zudo",
  "version": "4.4.0",
  "homepage": "https://doki.space",
  "license": "MIT",
  "main": "lib/main/index.js",
  "repository": "https://github.com/DokiDokiModManager/Mod-Manager",
  "bugs": "zudo@doki.space",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build-typescript": "tsc",
    "start": "yarn build-typescript && electron .",
    "start-dev": "yarn build-typescript && electron --inspect=5858 .",
    "pre-release": "rimraf dist lib && yarn build-typescript && node build/bundle-ui.js",
    "release-global": "yarn pre-release && electron-builder --config electron-builder.json --publish=always",
    "release-china": "yarn pre-release && electron-builder --config electron-builder-china.json --publish=always"
  },
  "dependencies": {
    "7zip-bin": "^5.0.3",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@sentry/electron": "1.5.2",
    "archiver": "^5.0.0",
    "chalk": "^4.1.0",
    "chmodr": "^1.2.0",
    "datauri": "^2.0.0",
    "discord-rich-presence": "^0.0.8",
    "diskusage": "^1.1.3",
    "du": "^1.0.0",
    "electron-updater": "^4.3.4",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "fuse.js": "^6.4.1",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0",
    "semver": "^7.1.1",
    "unrar-binaries": "^1.0.2",
    "vue": "^2.6.11",
    "vuex": "^3.5.1",
    "yauzl": "latest"
  },
  "devDependencies": {
    "@sentry/cli": "^1.55.1",
    "@types/node": "12.12.53",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
